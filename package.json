{
  "name": "Express-Lazyly",
  "version": "1.0.0",
  "description": "Express + MySQL + apidoc + pm2",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development nodemon app.js",
    "start:test": "cross-env NODE_ENV=test node app.js && pm2 start pm2.conf.json",
    "start:pro": "cross-env NODE_ENV=production node app.js && pm2 start pm2.conf.json",
    "apidoc": "apidoc -i app/routes -o public/apidoc"
  },
